# ===========================================
# Supabase Configuration
# ===========================================
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# ===========================================
# Server Configuration
# ===========================================
PORT=4001
NODE_ENV=development

# ===========================================
# Redis (for job queue)
# ===========================================
REDIS_URL=redis://localhost:6379

# ===========================================
# Scraper Configuration
# ===========================================
# Cron schedule for automatic scraping (default: every 6 hours)
SCRAPER_SCHEDULE="0 */6 * * *"

# Maximum concurrent browser instances
SCRAPER_CONCURRENCY=3

# Rate limits per platform (requests per minute)
SCRAPER_RATE_LIMIT_AIRBNB=10
SCRAPER_RATE_LIMIT_BOOKING=8
SCRAPER_RATE_LIMIT_EXPAT_DAKAR=20
SCRAPER_RATE_LIMIT_MAMAISON=20
SCRAPER_RATE_LIMIT_KEUR_IMMO=20
SCRAPER_RATE_LIMIT_COINAFRIQUE=20
SCRAPER_RATE_LIMIT_JUMIA_HOUSE=20

# ===========================================
# Proxy Configuration (optional)
# ===========================================
# PROXY_URL=http://proxy.example.com:8080
# PROXY_USERNAME=user
# PROXY_PASSWORD=pass

# ===========================================
# Intelligence Service URL
# ===========================================
INTELLIGENCE_SERVICE_URL=http://localhost:5000

# ===========================================
# Puppeteer Configuration
# ===========================================
# Set to 'true' to run browser in headless mode
PUPPETEER_HEADLESS=true

# Custom Chrome executable path (optional)
# PUPPETEER_EXECUTABLE_PATH=/usr/bin/google-chrome
