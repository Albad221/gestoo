{
  "name": "@gestoo/scraper-service",
  "version": "1.0.0",
  "description": "Web scraper for detecting illegal/unregistered accommodation listings",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "scrape": "tsx src/cli.ts",
    "batch-scrape": "tsx src/workers/batch-scraper.ts",
    "analyze": "tsx src/analysis/llm-analyzer.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.3.1",
    "node-cron": "^3.0.3",
    "openai": "^4.104.0",
    "p-queue": "^8.0.1",
    "puppeteer": "^21.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@vitest/coverage-v8": "^1.2.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  }
}
